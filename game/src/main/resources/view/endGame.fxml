<AnchorPane prefHeight="800.0" prefWidth="1550.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.EndGame">
   <children>
      <ImageView fitHeight="266.0" fitWidth="1550.0" pickOnBounds="true">
         <image>
            <Image url="@images/background1.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="272.0" fitWidth="1550.0" layoutY="264.0" pickOnBounds="true">
         <image>
            <Image url="@images/background2.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="268.0" fitWidth="1550.0" layoutY="532.0" pickOnBounds="true">
         <image>
            <Image url="@images/background3.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="83.0" fitWidth="51.0" layoutX="1253.0" layoutY="28.0" onMouseClicked="#exit" pickOnBounds="true" preserveRatio="true" styleClass="hand-cursor">
         <image>
            <Image url="@images/exit2.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="300.0" fitWidth="400.0" layoutX="607.0" layoutY="284.0" pickOnBounds="true" preserveRatio="true">
